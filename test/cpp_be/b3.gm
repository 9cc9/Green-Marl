// Test  of Collection Iteration
Proc P(G:Graph, A,B: N_P<Int>(G))
{
    Node_Set(G) S;
    Node_Seq(G) Q;
    Node_Order(G) O;
    Int X=0;

    Foreach(u: S.Items) {
        X += u.A;
    }

    For(v: O-.Items) {  // In reverse order
        X += v.A;
    }

    For(z: Q.Items) { 
        X += z.A;
    }
}
