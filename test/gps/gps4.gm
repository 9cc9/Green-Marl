
// Test of Communication
Procedure gps3 (G: Graph, A,B:N_P<Int>(G) ) 
{
    Int x=0;
    Foreach(s:G.Nodes)
    {
        Foreach(t:s.Nbrs) 
        {
            t.A += 1;           // Count number of in-neighbors
        }
        x += 1;
    }

    Foreach(s:G.Nodes)
    {
        Foreach(t:s.Nbrs) 
        {
            t.A += s.B + x + 1;           // Count number of in-neighbors
        }
    }
}
